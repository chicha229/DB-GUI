CREATE OR ALTER PROCEDURE UI$FORM_PANEL_CR_D (
    I_ID INTEGER)
RETURNS (
    ID INTEGER,
    FORM VARCHAR(30),
    CAPTION VARCHAR(100),
    PARENT INTEGER,
    ALIGN VARCHAR(30),
    INDEX_ON_PARENT INTEGER)
AS
 BEGIN
  FOR
    SELECT T.ID,T.FORM,T.CAPTION,T.PARENT,T.ALIGN,T.INDEX_ON_PARENT
    FROM UI$FORM_PANEL T 
    WHERE
      T.ID = :I_ID AND
      1=1
    INTO :ID,:FORM,:CAPTION,:PARENT,:ALIGN,:INDEX_ON_PARENT
  DO SUSPEND;
END