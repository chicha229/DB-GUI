CREATE OR ALTER PROCEDURE UI$PROCEDURE_CR_D (
    I_ID VARCHAR(30))
RETURNS (
    ID VARCHAR(30),
    NAME VARCHAR(100),
    CUSTOM_CLASS VARCHAR(100),
    PROCEDURE_NAME VARCHAR(30),
    IS_GRID_FOR_TABLE VARCHAR(30),
    IS_MODAL SMALLINT,
    FORCE_SAVE SMALLINT,
    GRID_STYLE VARCHAR(30))
AS
begin
  for select T.ID, T.PROCEDURE_NAME, T.IS_GRID_FOR_TABLE, B.NAME, B.CUSTOM_CLASS, B.IS_MODAL, T.FORCE_SAVE,
             T.GRID_STYLE
      from UI$PROCEDURE T
      join UI$BLOCK B on B.ID = T.ID
      where T.ID = :I_ID and
            1 = 1
      into :ID, :PROCEDURE_NAME, :IS_GRID_FOR_TABLE, :NAME, :CUSTOM_CLASS, :IS_MODAL, :FORCE_SAVE, :GRID_STYLE
  do
    suspend;
end