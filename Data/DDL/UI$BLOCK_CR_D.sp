CREATE OR ALTER PROCEDURE UI$BLOCK_CR_D (
    I_ID VARCHAR(30))
RETURNS (
    ID VARCHAR(30),
    BLOCK_TYPE VARCHAR(30),
    NAME VARCHAR(100),
    CUSTOM_CLASS VARCHAR(100),
    IS_MODAL SMALLINT)
AS
 BEGIN
  FOR
    SELECT T.ID,T.BLOCK_TYPE,T.NAME,T.CUSTOM_CLASS,T.IS_MODAL
    FROM UI$BLOCK T 
    WHERE
      T.ID = :I_ID AND
      1=1
    INTO :ID,:BLOCK_TYPE,:NAME,:CUSTOM_CLASS,:IS_MODAL
  DO SUSPEND;
END