CREATE OR ALTER PROCEDURE UI$MENU_ITEM_INS (
    I_ID INTEGER,
    I_NAME VARCHAR(100),
    I_ITEM_TYPE VARCHAR(30),
    I_PARENT INTEGER,
    I_BLOCK VARCHAR(30),
    I_ORDER_NUM INTEGER)
AS
 BEGIN
  INSERT INTO UI$MENU_ITEM (ID,NAME,ITEM_TYPE,PARENT,BLOCK,ORDER_NUM)
  VALUES  (:I_ID,:I_NAME,:I_ITEM_TYPE,:I_PARENT,:I_BLOCK,:I_ORDER_NUM);
END